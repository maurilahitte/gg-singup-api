{
    "microservice_name": "karvi-api-template",
    "secret_target": {
        "dev_ar": "dev_ar-karvi-api-template-ID-AWS",
        "stg_ar": "stg_ar-karvi-api-template-ID-AWS",
        "prd_ar": "prd_ar-karvi-api-template-ID-AWS",
        "prd_br": "prd_br-karvi-api-template-ID-AWS"
    },
    "env": [
        {
            "path_name": "EXAMPLE",
            "value": {
                "dev_ar": "EXAMPLE_DEV",
                "stg_ar": "EXAMPLE_STG",
                "prd_ar": "EXAMPLE_PROD",
                "prd_br": "EXAMPLE_PROD"
            },
            "description": "EXAMPLE"
        },

        {
            "path_name": "NODE_ENV",
            "value": {
                "dev_ar": "development",
                "stg_ar": "staging",
                "prd_ar": "production",
                "prd_br": "production"
            },
            "description": "NODE_ENV"
        },
        {
            "path_name": "APP_PORT",
            "value": {
                "dev_ar": "3300",
                "stg_ar": "3300",
                "prd_ar": "3300",
                "prd_br": "3300"
            },
            "description": "APP_PORT"
        },
        {
            "path_name": "CACHE_DEFAULT_TTL",
            "value": {
                "dev_ar": "600",
                "stg_ar": "600",
                "prd_ar": "600",
                "prd_br": "600"
            },
            "description": "CACHE_DEFAULT_TTL"
        },
        {
            "path_name": "CORS_ORIGINS",
            "value": {
                "dev_ar": "*",
                "stg_ar": "*",
                "prd_ar": "*",
                "prd_br": "*"
            },
            "description": "CORS_ORIGINS"
        },
        {
            "path_name": "SENTRY_DSN",
            "value": {
                "dev_ar": "{{ SENTRY_DSN }}",
                "stg_ar": "{{ SENTRY_DSN }}",
                "prd_ar": "{{ SENTRY_DSN }}",
                "prd_br": "{{ SENTRY_DSN }}"
            },
            "description": "SENTRY_DSN"
        }
    ]
}
